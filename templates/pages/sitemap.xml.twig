<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url><loc>{{ url('home') }}</loc></url>
    <url><loc>{{ url('project_list') }}</loc></url>
    <url><loc>{{ url('article_list') }}</loc></url>
    {% for article in articles %}
